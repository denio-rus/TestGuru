<tr> 
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td style="text-align:center"><%= test.questions.count %></td>
  <td><%= link_to t('link_show'), admin_test_path(test) %></td>
  <td><%= link_to t('link_edit'), edit_admin_test_path(test) %>
  <td>
    <%= link_to t('link_delete'),
                admin_test_path(test),
                method: :delete,
                data:   { confirm: t(:sure?) } %>
  </td>
</tr>